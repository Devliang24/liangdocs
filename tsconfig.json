{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"]
  },
   /* 指定编译文件或排除指定编译文件 */
  "include": [  
    "./docs/.vitepress/components/*.vue",
    "./docs/.vitepress/components.d.ts",
    "./docs/.vitepress/auto-imports.d.ts"
  ],
  "exclude": ["**/node_modules/**", "**/dist/**"]
}
