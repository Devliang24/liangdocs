{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "preserveValueImports": true,
    "importsNotUsedAsValues": "error",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"]
  },
   /* 指定编译文件或排除指定编译文件 */
  "include": [  
    "docs",
    "docs/.vitepress/components/*.vue",
    "docs/.vitepress/*.ts",
    "scripts",
  ],
  "exclude": ["**/node_modules/**", "**/dist/**"]
}
